# Phoenix Real Estate Data Collector Environment Variables
# Copy this file to .env and fill in your actual values

# Environment (dev, test, prod)
ENV=dev

# Database Credentials
CREDENTIAL_DB_HOST=localhost
CREDENTIAL_DB_PORT=27017
CREDENTIAL_DB_USER=your_db_user
CREDENTIAL_DB_PASS=your_db_password
CREDENTIAL_DB_NAME=phoenix_real_estate

# Proxy Credentials (if using proxies)
CREDENTIAL_PROXY_HOST=proxy.example.com
CREDENTIAL_PROXY_PORT=8080
CREDENTIAL_PROXY_USER=proxy_user
CREDENTIAL_PROXY_PASS=proxy_password

# API Keys
SECRET_API_KEY_GOOGLE=your_google_api_key
SECRET_API_KEY_AZURE=your_azure_api_key
SECRET_API_KEY_OPENAI=your_openai_api_key

# MongoDB Atlas Connection (example with base64 encoding)
# You can encode sensitive values with base64 by prefixing with 'b64:'
# Example: echo -n "mongodb+srv://user:pass@cluster.mongodb.net" | base64
SECRET_MONGODB_URI=b64:bW9uZ29kYitzcnY6Ly91c2VyOnBhc3NAY2x1c3Rlci5tb25nb2RiLm5ldA==

# Security Settings
SECRET_JWT_SECRET_KEY=your_jwt_secret_key
SECRET_ENCRYPTION_KEY=your_encryption_key

# Base64 encoded secrets (prefix with b64:)
SECRET_ENCODED_VALUE=b64:c29tZV9lbmNvZGVkX3ZhbHVl

# Maricopa County API
SECRET_MARICOPA_API_KEY=your_maricopa_api_key

# Phoenix MLS Search Credentials
CREDENTIAL_MLS_USERNAME=your_mls_username
CREDENTIAL_MLS_PASSWORD=your_mls_password

# Other Configuration (non-secret)
DEBUG=true
LOG_LEVEL=INFO
MAX_RETRY_ATTEMPTS=3
REQUEST_TIMEOUT=30
# Phoenix Real Estate Configuration

# Environment
ENVIRONMENT=development

# MongoDB
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/
MONGODB_DATABASE=phoenix_real_estate

# API Keys
MARICOPA_API_KEY=your_api_key_here
PHOENIX_MLS_API_KEY=your_api_key_here

# LLM Configuration
LLM_MODEL=llama2
LLM_API_URL=http://localhost:11434

# Proxy Configuration
PROXY_ENABLED=false
PROXY_ROTATION_INTERVAL=300

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Collection Settings
COLLECTION_BATCH_SIZE=100
COLLECTION_RETRY_ATTEMPTS=3
COLLECTION_TIMEOUT=30

# Processing Settings
PROCESSING_QUEUE_SIZE=1000
PROCESSING_WORKERS=4
